<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog id="fileUpload" class="sapUiContentPadding" contentWidth="400px" contentHeight="130px" title="Chartering Request" >
        <content>
            <Text text="This application needs a file to upload to SAP System"></Text>
                <HBox class="MastDialogButtonAlign">
                <Button text="Ok" class="masterbuttonsalign"></Button>
                <Button text="Exit" class="masterbuttonsalign" press="exitDialog"></Button>
            </HBox>
        </content>
    </Dialog>
</core:FragmentDefinition>