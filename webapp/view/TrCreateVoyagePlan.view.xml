<mvc:View xmlns:semantic="sap.m.semantic" xmlns:core="sap.ui.core" xmlns:tnt="sap.tnt" xmlns:cards="sap.f.cards"
xmlns:form="sap.ui.layout.form"
 controllerName="nauticalfe.controller.TrCreateVoyagePlan"
    xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true">
    <Bar id="navbar">
      <contentLeft>
        <Button id="Button1" text="Save" width="100px" class="planNav"/>
        <Button id="Button2" text="Refresh" width="100px" class="planNav"/>
        <Button id="Button3" text="Frieght Simulator" width="150px" class="planNav"/>
        <Button id="Button4" text="Copy" width="100px" class="planNav"/>
        <Select id="Select1" width="100px" >
            <items>
                <core:Item id="_IDGenItem1" text="more"/>
            </items>
        </Select>
      </contentLeft>
    </Bar>
    <Text text="Voyage Details" id="detailsheading"/>
    <HBox id="_IDGenHBox1" >
    <VBox id="Row1" >
            <HBox id="_IDGenHBox2" >
            <Label id="Label1" text="Voyage No.:"/>
            <Input id="Input1" width="200px"/>
            </HBox>
        
            <HBox id="_IDGenHBox3" >
            <Label id="Label2" text="Voyage Name:"/>
            <Input id="Input2" width="200px"/>
            </HBox>
       
    </VBox>
    <VBox id="Row2">
            <HBox id="_IDGenHBox4" >
            <Label id="Label3" text="Voyage Type:"/>
            <Select id="Select2" width="200px">
                <items>
                    <core:Item id="Item2" text="Voyage Type"/>
                </items>
            </Select>
            </HBox>
        
            <HBox id="_IDGenHBox5" >
            <Label id="_Label4" text="Vessel Type:"/>
            <Select id="_Select3" width="200px">
                <items>
                    <core:Item id="_Item3" text="Vessel Type"/>
                </items>
            </Select>
            </HBox>
      
    </VBox>
    <VBox id="Row3">
            <HBox id="_IDGenHBox6" >
            <Label id="Label5" text="Currency:  "/>
            <Input id="Input3" width="100px"/>
            </HBox>
       
            <HBox id="_IDGenHBox7" >
            <Label id="Label6" text="Historical Frieght Cost Name:"/>
            <Input id="Input4" width="200px"/>
            </HBox>
      
    </VBox>
    <VBox id="Row4">
            <HBox id="_IDGenHBox8" >
            <Label id="Label7" text="Bid Type:  "/>
            <Input id="Input5" width="100px"/>
            </HBox>
       
            <HBox id="_IDGenHBox9" >
            <Label id="Label8" text="Status"/>
            <Input id="Input6" width="200px"/>
            </HBox>
      
    </VBox>
    </HBox>

<VBox id="_IDGenVBox1"
		class="sapUiSmallMargin">
		<HBox id="_IDGenHBox10">
		
			<Button id="_IDGenButton5"
				text="Vessel Details"
				press="handleNav">
				<layoutData>
					<FlexItemData id="_IDGenFlexItemData1" growFactor="1" />
				</layoutData>
				<customData>
					<core:CustomData
						key="target"
						value="p1" />
				</customData>
			</Button>
			<Button id="_IDGenButton6"
				text="Vessel Plan/Actual"
				press="handleNav">
				<layoutData>
					<FlexItemData id="_IDGenFlexItemData2" growFactor="1" />
				</layoutData>
				<customData>
					<core:CustomData
						key="target"
						value="p2" />
				</customData>
			</Button>
			<Button id="_IDGenButton7"
				text="Bid Details"
				press="handleNav">
				<layoutData>
					<FlexItemData id="_IDGenFlexItemData3" growFactor="1" />
				</layoutData>
				<customData>
					<core:CustomData
						key="target"
						value="p3" />
				</customData>
			</Button>
			<Button id="_IDGenButton8"
				text="Long Text"
				press="handleNav">
				<layoutData>
					<FlexItemData id="_IDGenFlexItemData4" growFactor="1" />
				</layoutData>
				<customData>
					<core:CustomData
						key="target"
						value="p4" />
				</customData>
			</Button>
			
		</HBox>

		<HBox id="_IDGenHBox11">
			<Button id="_IDGenButton9"
				text="Back"
				type="Back"
				press="handleNav">
				<layoutData>
					<FlexItemData id="_IDGenFlexItemData5" growFactor="1" />
				</layoutData>
			</Button>
			<Select id="animationSelect">
				<core:Item id="_IDGenItem2" text="Slide animation" key="slide" />
				<core:Item id="_IDGenItem3" text="Base slide animation" key="baseSlide" />
				<core:Item id="_IDGenItem4" text="Fade animation" key="fade" />
				<core:Item id="_IDGenItem5" text="Flip animation" key="flip" />
				<core:Item id="_IDGenItem6" text="Show animation" key="show" />
				<layoutData>
					<FlexItemData id="_IDGenFlexItemData6" growFactor="1" />
				</layoutData>
			</Select>
		</HBox>


		<NavContainer
			navigationFinished="onNavigationFinished"
			id="navCon"
			width="100px"
			height="16em"
			class="navContainerControl sapUiSmallMarginBottom">
			<Page
				id="p1"
				 >
				<Text text="hello"></Text>
				
				<footer><OverflowToolbar id="_IDGenOverflowToolbar1"><Button id="_IDGenButton1" text="Action 1" /></OverflowToolbar></footer>
			</Page>
			<Page
				id="p2"
				title="Page 2" >
				<footer><OverflowToolbar id="_IDGenOverflowToolbar2"><Button id="_IDGenButton2" text="Action 2" /></OverflowToolbar></footer>
			</Page>
			<Page
				id="p3"
				title="Page 3" >
				<footer><OverflowToolbar id="_IDGenOverflowToolbar3"><Button id="_IDGenButton3" text="Action 3" /></OverflowToolbar></footer>
			</Page>
			<Page
				id="p4"
				title="Page 4" >
				
				<footer><OverflowToolbar id="_IDGenOverflowToolbar4"><Button id="_IDGenButton4" text="Action 4" /></OverflowToolbar></footer>
			</Page>
		</NavContainer>

		
	</VBox>

</mvc:View>
