<mvc:View xmlns:unified="sap.ui.unified" xmlns:core="sap.ui.core" xmlns:tnt="sap.tnt"
xmlns:form="sap.ui.layout.form"
 xmlns:l="sap.ui.layout"
 controllerName="nauticalfe.controller.TrRequestQuotation"
    xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true">
    <mvc:content>
        <Bar id="_IDGenBar1">
        <contentLeft>
            <Button id="_IDGenButton1" text="Submit Quotation"></Button>
	        <Button id="button" icon="sap-icon://slim-arrow-down" iconFirst="false" text="More" press="onPress" ariaHasPopup="Menu"/>
        </contentLeft>
        </Bar>
            <Text id="_IDGenText1" text="Bidding Details" class="heading "></Text>
            <FlexBox id="_IDGenFlexBox1" class="quoteoptions">
               <VBox id="_IDGenVBox1" alignItems="End">
                    <HBox id="_IDGenHBox1" alignItems="End" class="optionsspacing">
                        <Label id="_IDGenLabel1" text="Chartering Request No :" class="customRequiredLabel" width="150px"></Label>
                        <Input id="_IDGenInput1" width="170px"></Input>
                        <Button id="_IDGenButton3" icon="sap-icon://copy" press="openCharteringReq"></Button>
                    </HBox>
                    <HBox id="_IDGenHBox2" alignItems="End"  class="optionsspacing">
                        <Label id="_IDGenLabel2" text="Bidding Start Date:" class="customRequiredLabel"></Label>
                        <DatePicker id="_IDGenDatePicker1" width="200px" placeholder="MM:DD:YY"></DatePicker>
                    </HBox>
                    <HBox id="_IDGenHBox3" alignItems="End"  class="optionsspacing">
                         <Label id="_IDGenLabel3" text="Bidding Start Time:" class="customRequiredLabel"></Label>
                         <TimePicker id="_IDGenTimePicker1" width="200px" placeholder="00:00:00"></TimePicker>
                    </HBox>
                    <HBox id="_IDGenHBox4" alignItems="End"  class="optionsspacing">
                         <Label id="_IDGenLabel4" text="Bidding End Date:" class="customRequiredLabel"></Label>
                         <DatePicker id="_IDGenDatePicker2" width="200px" placeholder="MM:DD:YY"></DatePicker>
                    </HBox>
                    <HBox id="_IDGenHBox5" alignItems="Center" class="optionsspacing">
                        <Label id="_IDGenLabel5" text="Bidding End Time:"  class="customRequiredLabel"></Label>
                        <TimePicker id="_IDGenTimePicker2" width="200px" placeholder="00:00:00"></TimePicker>
                    </HBox>
               </VBox>
            </FlexBox>
    </mvc:content>
   </mvc:View> 